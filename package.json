{
  "name": "@yakiyo/cache-js",
  "version": "2.0.1",
  "description": "Minimal implementation of a cache using javascript Map",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/typings",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yakiyo/cache-js.git"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "test": "node test/test.cjs && node test/test.mjs",
    "build": "rm -rf lib/ && tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "husk": "husky install",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run lint && npm run build",
    "setup": "npm run build && npm run husk",
    "pretty:w": "prettier --write **/*.ts",
    "pretty:c": "prettier --check **/*.ts",
    "lint-stage": "lint-staged"
  },
  "keywords": [
    "cache",
    "map",
    "package"
  ],
  "author": "Yakiyo",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7"
  },
  "files": [
    "lib/"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": true,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "endOfLine": "lf"
  },
  "lint-staged": {
    "*.ts": ["npm run lint", "npm run pretty:w"]
  }
}
